{
  "name": "react-webpack",
  "version": "1.0.0",
  "description": "React application configured manually with Webpack including HMR, SAAS, TS, SVG, IMG and linters support",
  "main": "src/index.tsx",
  "scripts": {
    "dev": "webpack-dev-server --open",
    "build": "webpack",
    "lint:prettier": "prettier --check .",
    "fix:prettier": "prettier --write .",
    "lint:eslint": "eslint --ext .ts,.tsx,.js .",
    "fix:eslint": "eslint --ext .ts,.tsx,.js . --fix",
    "lint:stylelint": "stylelint **/*.{scss,css,tsx}",
    "fix:stylelint": "stylelint --fix **/*.{scss,css,tsx}"
  },
  "repository": {
    "type": "git",
    "url": "react-webpack"
  },
  "keywords": [
    "react",
    "webpack",
    "HMR",
    "configuration"
  ],
  "author": "gaidukov200078@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "7.20.12",
    "@babel/preset-env": "7.20.2",
    "@babel/preset-react": "7.18.6",
    "@svgr/webpack": "6.5.1",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "@typescript-eslint/parser": "5.49.0",
    "babel-loader": "9.1.2",
    "babel-plugin-styled-components": "2.0.7",
    "css-loader": "6.7.3",
    "eslint": "8.32.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-react": "7.32.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "html-webpack-plugin": "5.5.0",
    "image-minimizer-webpack-plugin": "3.8.1",
    "imagemin": "8.0.1",
    "imagemin-gifsicle": "7.0.0",
    "imagemin-jpegtran": "7.0.0",
    "imagemin-mozjpeg": "10.0.0",
    "imagemin-optipng": "8.0.0",
    "imagemin-pngquant": "9.0.2",
    "postcss-loader": "7.0.2",
    "prettier": "2.8.3",
    "sass": "1.57.1",
    "sass-loader": "13.2.0",
    "style-loader": "3.3.1",
    "stylelint": "14.16.1",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-prettier-scss": "0.0.1",
    "stylelint-config-standard": "29.0.0",
    "stylelint-config-standard-scss": "6.1.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-loader": "9.4.2",
    "typescript": "4.9.4",
    "webpack": "5.75.0",
    "webpack-cli": "5.0.1",
    "webpack-dev-server": "4.11.1"
  },
  "dependencies": {
    "copy-webpack-plugin": "11.0.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "5.3.6"
  }
}
